   "source": [
    "import math\n",
    "coins=[10,5,2,1]\n",
    "result=0\n",
    "amt=61\n",
    "for i in range (len(coins)):\n",
    "    if coins[i]<=amt:\n",
    "            value=math.floor(amt/coins[i])\n",
    "            result=result+value\n",
    "            print(value,\"coin\"+\" of\",coins[i])\n",
    "            amt=amt-value*coins[i]\n",
    "    if amt==0:\n",
    "        break\n"
   ]
